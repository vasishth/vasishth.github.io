@Article{OztekinMcElree2010,
  Title                    = {Relationship between measures of working memory capacity and the time course of short-term memory retrieval and interference resolution},
  Author                   = {\"{O}ztekin, Ilke and McElree, Brian},
  Journaltitle             = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
  Year                     = {2010},
  Doi                      = {10.1037/a0018029},
  ISSN                     = {1939-1285},
  Month                    = {March},
  Number                   = {2},
  Pages                    = {383--397},
  Url                      = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2872513\&tool=pmcentrez\&rendertype=abstract},
  Volume                   = {36},

  Abstract                 = {The response-signal speed-accuracy trade-off (SAT) procedure was used to investigate the relationship between measures of working memory capacity and the time course of short-term item recognition. High- and low-span participants studied sequentially presented 6-item lists, immediately followed by a recognition probe. Analyses of composite list and serial position SAT functions found no differences in retrieval speed between the 2 span groups. Overall accuracy was higher for high spans than low spans, with more pronounced differences for earlier serial positions. Analysis of false alarms to recent negatives (lures from the previous study list) revealed no differences in the timing or magnitude of early false alarms, thought to reflect familiarity-based judgments. However, analyses of false alarms later in retrieval indicated that recollective information accrues more slowly for low spans, which suggests that recollective information may also contribute less to judgments concerning studied items for low-span participants. These findings can provide an explanation for the greater susceptibility of low spans to interference.},
  File                     = {:home/bruno/Documents/Papers/\\"{O}ztekin, McElree - 2010 - Relationship between measures of working memory capacity and the time course of short-term memory retrieval a.pdf:PDF},
  Keywords                 = {Attention,Attention: physiology,Humans,Memory,Mental Processes,Mental Processes: physiology,Mental Recall,Mental Recall: physiology,Neuropsychological Tests,Nonlinear Dynamics,Phd1,Reaction Time,Short-Term,Short-Term: physiology,Time Factors,interference,wmc},
  Mendeley-tags            = {Phd1,interference,wmc},
  Pmid                     = {20192537}
}

