@Article{Berman2009,
  Title                    = {In search of decay in verbal short-term memory},
  Author                   = {Berman, Marc G. and Jonides, John and Lewis, Richard L.},
  Journaltitle             = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
  Year                     = {2009},
  Doi                      = {10.1037/a0014873},
  ISSN                     = {0278-7393},
  Month                    = {March},
  Number                   = {2},
  Pages                    = {317--333},
  Url                      = {http://dx.doi.org/10.1037/a0014873},
  Volume                   = {35},

  Abstract                 = {Is forgetting in the short term due to decay with the mere passage of time, interference from other memoranda, or both? Past research on short-term memory has revealed some evidence for decay and a plethora of evidence showing that short-term memory is worsened by interference. However, none of these studies has directly contrasted decay and interference in short-term memory in a task that rules out the use of rehearsal processes. In this article the authors present a series of studies using a novel paradigm to address this problem directly, by interrogating the operation of decay and interference in short-term memory without rehearsal confounds. The results of these studies indicate that short-term memories are subject to very small decay effects with the mere passage of time but that interference plays a much larger role in their degradation. The authors discuss the implications of these results for existing models of memory decay and interference.},
  Citation-verif           = {pages; year; volume; number; issn; journaltitle; url; title; doi; publisher; author; were verified from given doi},
  File                     = {:/home/bruno/Documents/Papers/Berman, Jonides, Lewis - 2009 - In search of decay in verbal short-term memory.pdf:PDF},
  Keywords                 = {Adult,Attention,Female,Humans,Male,Memory,Paired-Associate Learning,Phd1,Practice (Psychology),Proactive Inhibition,Reaction Time,Recognition (Psychology),Semantics,Short-Term,Verbal Learning,Young Adult,wmc},
  Mendeley-tags            = {Phd1,wmc},
  Pmid                     = {19271849},
  Publisher                = {American Psychological Association (APA)}
}

