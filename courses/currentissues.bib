@Article{YeZhou2009,
  Title                    = {{Conflict control during sentence comprehension: fMRI evidence}},
  Author                   = {Ye, Zheng and Zhou, Xiaolin},
  Journaltitle             = {NeuroImage},
  Year                     = {2009},
