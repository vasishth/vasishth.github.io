@Article{KupermanVanDyke2011,
  Title                    = {Effects of individual differences in verbal skills on eye-movement patterns during sentence reading},
  Author                   = {Kuperman, Victor and {Van Dyke}, Julie A.},
  Journaltitle             = {Journal of Memory and Language},
