  Keywords                 = {Acoustic Stimulation,Adolescent,Adult,Auditory,Auditory: physiology,Brain,Brain Mapping,Brain: physiology,Cognition,Cognition: physiology,Comprehension,Comprehension: physiology,Electroencephalography,Evoked Potentials,Female,Humans,Individuality,Male,Memory,Phd1,Phd1 cognitive\_control,Regression Analysis,Semantics,Short-Term,Short-Term: physiology,Speech,Speech: physiology,Verbal Learning,Verbal Learning: physiology,Young Adult,cognitive\_control,expectations,lexical\_expectations,linguistics,predictions},
