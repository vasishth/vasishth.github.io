@Article{Kieffaber2013,
  Title                    = {Dissociating stimulus-set and response-set in the context of task-set switching},
  Author                   = {Kieffaber, Paul D. and Kruschke, John K. and Cho, Raymond Y. and Walker, Philip M. and Hetrick, William P.},
