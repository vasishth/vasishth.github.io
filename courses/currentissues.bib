@Article{KathleenPichora-Fuller2008,
  Title                    = {Use of supportive context by younger and older adult listeners: balancing bottom-up and top-down information processing},
  Author                   = {{Kathleen Pichora-Fuller}, M},
  Journaltitle             = {International Journal of Audiology},
  Year                     = {2008},
  Doi                      = {10.1080/14992020802307404},
  ISSN                     = {1708-8186},
  Month                    = {November},
  Pages                    = {72--82},
  Url                      = {http://www.ncbi.nlm.nih.gov/pubmed/19012114},
  Volume                   = {47 Suppl 2},

  Abstract                 = {Older adults often have more difficulty listening in challenging environments than their younger adult counterparts. On the one hand, auditory aging can exacerbate and/or masquerade as cognitive difficulties when auditory processing is stressed in challenging listening situations. On the other hand, an older listener can overcome some auditory processing difficulties by deploying compensatory cognitive processing, especially when there is supportive context. Supportive context may be provided by redundant cues in the external signal(s) and/or by internally stored knowledge about structures that are functionally significant in communication. It seems that listeners may achieve correct word identification in various ways depending on the challenges and supports available in complex auditory scenes. We will review evidence suggesting that older adults benefit as much or more than younger adults from supportive context at multiple levels where expectations or constraints may be related to redundancies in semantic, syntactic, lexical, phonological, or other sub-phonemic cues in the signal, and/or to expert knowledge of structures at these levels.},
  File                     = {:/home/bruno/Documents/Papers/Kathleen Pichora-Fuller - 2008 - Use of supportive context by younger and older adult listeners balancing bottom-up and top-down informa.pdf:PDF},
  Keywords                 = {Adaptation,Adult,Age Factors,Aging,Aging: physiology,Auditory Pathways,Auditory Pathways: physiology,Cognition,Comprehension,Cues,Humans,Pattern Recognition,Phd1,Physiological,Speech Intelligibility,Speech Perception,TOREAD,expectations,lexical\_expectations,predictions},
  Mendeley-tags            = {Phd1,TOREAD,expectations,lexical\_expectations,predictions},
  Pmid                     = {19012114}
}

@Article{Kelley2011,
  Title                    = {Working memory load modulates distractor competition in primary visual cortex},
  Author                   = {Kelley, Todd A. and Lavie, Nilli},
  Journaltitle             = {Cerebral Cortex},
  Year                     = {2011},
  Doi                      = {10.1093/cercor/bhq139},
  ISSN                     = {1460-2199},
  Month                    = {Mar},
  Number                   = {3},
  Pages                    = {659--665},
  Url                      = {http://dx.doi.org/10.1093/cercor/bhq139},
  Volume                   = {21},

  Abstract                 = {When stimuli compete for sensory processing and response selection, coherent goal-guided behavior requires cognitive control so that task-relevant "targets" rather than irrelevant distractors are selected. It has been shown that reduced cognitive control under high working memory load increases distractor competition for selection. It remains unknown, though, whether cognitive control by working memory has an effect on the earliest levels of sensory processing in primary visual cortex. The present study addressed this question by having subjects perform a selective attention task involving classification of meaningful target objects while also ignoring congruent and incongruent distractor images. The level of cognitive control over distractor competition was varied through a concurrent working memory task of either low (1 digit) or high (6 digits) load. Functional magnetic resonance imaging revealed greater distractor competition effects not only on behavior but also on the sensory correlates in primary visual cortex (areas V1-V2) in conditions of high (vs. low) working memory load. In addition, high working memory load resulted in increased congruency-related functional connectivity between anterior cingulate cortex and V1. These results are the first to establish the neural correlates of distractor competition effects in primary visual cortex and the critical role of working memory in their cognitive control.},
  Citation-verif           = {pages; year; volume; month; number; issn; journaltitle; url; title; doi; publisher; author; were verified from given doi},
  File                     = {:/home/bruno/Documents/Papers/Kelley, Lavie - 2011 - Working memory load modulates distractor competition in primary visual cortex.pdf:PDF},
  Keywords                 = {Adolescent,Adult,Attention,Attention: physiology,Brain Mapping,Computer-Assisted,Female,Humans,Image Processing,Magnetic Resonance Imaging,Male,Memory,Phd1,Short-Term,Short-Term: physiology,Visual Cortex,Visual Cortex: physiology,Young Adult,cognitive\_control,stroop},
  Mendeley-tags            = {Phd1,cognitive\_control,stroop},
  Pmid                     = {20699229},
  Publisher                = {Oxford University Press (OUP)}
}

