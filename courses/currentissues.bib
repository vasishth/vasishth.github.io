  File                     = {:/home/bruno/Documents/Papers/Staub - 2010 - Eye movements and processing difficulty in object relative clauses.pdf:PDF},
  Keywords                 = {Caro,Eye Movements,Eye Movements: physiology,Female,Fixation,Humans,Male,Ocular,Ocular: physiology,Phd1,Psycholinguistics,Psychomotor Performance,Psychomotor Performance: physiology,Reading,Semantics,Young Adult,locality},
  Mendeley-tags            = {Caro,Phd1,locality},
  Pmid                     = {20427040},
  Publisher                = {Elsevier BV}
}

@Article{StaubClifton2006,
